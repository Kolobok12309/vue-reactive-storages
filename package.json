{
  "name": "vue-reactive-storages",
  "version": "1.0.13",
  "description": "Plugin for vue and local/sessionStorage",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "tsc && vue-cli-service build --target lib --name vue-reactive-storages src/index.ts",
    "lint": "vue-cli-service lint"
  },
  "main": "lib/src/index.js",
  "module": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "unpkg": "dist/vue-reactive-storages.umd.min.js",
  "dependencies": {
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.3",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "lint-staged": "^8.2.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "vue-cli-plugin-typescript": "0.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "homepage": "https://github.com/Kolobok12309/vue-reactive-storages",
  "keywords": [
    "vue",
    "localstorage",
    "sessionstorage",
    "reactive"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": "github:kolobok12309/vue-reactive-storages"
}
